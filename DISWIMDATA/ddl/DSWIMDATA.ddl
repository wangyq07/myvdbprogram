INNER JOIN DSWIMDATASRC.BE_ENV_TEST BT ON SD.OBJECT_ID = BT.BE_TEST_ID
INNER JOIN DSWIMDATASRC.WELL W ON BT.ENTITY_ID = W.WELL_ID
