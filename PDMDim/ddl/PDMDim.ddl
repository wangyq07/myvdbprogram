FPS.OIL_ALLOCATED_VOLUME AS	OIL,
cast(FPS.OIL_VOLUME_UOM as string) AS	OIL_UOM,
FPS.GAS_ALLOCATED_VOLUME AS	GAS,
cast(FPS.GAS_VOLUME_UOM as string) AS	GAS_UOM,
