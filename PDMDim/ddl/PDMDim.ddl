WHERE cast(PR.W_WC_FAC_IND AS string) IN ('W') AND cast(DWC.PRODUCTION_METHOD AS string) IN ('Gas Assisted Plunger Lift','GasAssisted Plunger Lift','Gas Assisted Lift','GasAssistedPLift') 
