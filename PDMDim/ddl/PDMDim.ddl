SELECT KPI, KPI_DATE,SUM(KPI_VALUE) AS KPI_VALUE, DAH.LEVEL2_NAME AS LEVEL_NAME, DAH.LEVEL2_TYPE AS LEVEL_TYPE, DAH.LEVEL2_ID  AS LEVEL_ID , 'LEVEL2' AS LEVEL_ALIAS 
