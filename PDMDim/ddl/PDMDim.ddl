NULL AS PEAK_LOAD,NULL AS PEAK_LOAD_UOM,NULL AS MIN_LOAD,NULL AS MIN_LOAD_UOM,NULL AS RUNTIME,NULL AS RUNTIME_UOM,NULL AS SPM,NULL AS SPM_UOM,
