NULL AS	CASING_PRESSURE,NULL AS CASING_PRESS_UOM,NULL AS	TUBING_PRESSURE,NULL AS	TUBING_PRESS_UOM,NULL AS O_CASING_PRESSURE,NULL AS O_CASING_PRESS_UOM,NULL AS LINE_PRESSURE,
