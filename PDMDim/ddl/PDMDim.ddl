AS
(
 SELECT T.MEASUREMENT_DATE as MEASUREMENT_DATE , MAX(T.RUNTIME) RUNTIME, MAX(T.RUNTIME_UOM) RUNTIME_UOM, MAX(T.SURFACE_CARD_TYPE)SURFACE_CARD_TYPE,AVG(T.SPM) SPM, MAX(T.SPM_UOM) SPM_UOM,
